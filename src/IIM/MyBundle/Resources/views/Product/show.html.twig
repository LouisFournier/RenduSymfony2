{% extends 'IIMMyBundle::layout.html.twig' %}

{% block content %}

<div class="panel panel-info">
    <div class="panel-heading"><h1>File</h1></div>

    <table class="table table-striped">
        <tbody>
            <tr>
                <th>File</th>
                <td>{{ entity.file }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ entity.content }}</td>
            </tr>
            <tr>
                <th>FileName</th>
                <td>{{ entity.filename }}</td>
            </tr>


        </tbody>
    </table>


        <a href="{{ path('product') }}">
            <button type="button" class="btn btn-default">Back to the list</button>
        </a>


        <a href="{{ path('product_edit', { 'id': entity.id }) }}">
            <button type="button" class="btn btn-warning">Edit</button>
        </a>


        {{ form(delete_form) }}

</div>
{% endblock content %}
